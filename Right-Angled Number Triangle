import java.util.Scanner;
public class pattern4 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		/*Another Type
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(i+" ");
				 
			}System.out.print("\n");
		}*/
		for(int i=0;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print( j+" ");
			}System.out.print("\n");
		}
	}

}
